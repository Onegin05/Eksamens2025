RewriteEngine On
RewriteBase /3pt1/cebotars/ZalaAugsmeCebotars/

# Handle PHP files
<FilesMatch "\.php$">
    SetHandler application/x-httpd-php
</FilesMatch>

# Prevent directory listing
Options -Indexes

# Handle 404 errors
ErrorDocument 404 /3pt1/cebotars/ZalaAugsmeCebotars/404.php

# Enable PHP error reporting
php_flag display_errors on
php_value error_reporting E_ALL 